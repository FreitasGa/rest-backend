{
  "name": "rest-backend",
  "version": "1.0.0",
  "description": "",
  "main": "main.js",
  "type": "commonjs",
  "scripts": {
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "prestart": "prisma migrate deploy",
    "start": "node dist/main.js",
    "prebuild": "rimraf dist",
    "build": "swc src -d dist",
    "test": "cross-env NODE_ENV=test jest -c jest.config.json",
    "test:watch": "cross-env NODE_ENV=test jest -c jest.config.json --watch",
    "test:coverage": "cross-env NODE_ENV=test jest -c jest.config.json --coverage",
    "test:unit": "cross-env NODE_ENV=test jest -c jest.unit.config.json",
    "test:e2e": "cross-env NODE_ENV=test jest -c jest.e2e.config.json",
    "test:bug": "cross-env NODE_ENV=test jest -c jest.bug.config.json",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:write": "prettier --write \"src/**/*.ts\"",
    "lint:check": "eslint \"src/**/*.ts\"",
    "lint:write": "eslint --fix \"src/**/*.ts\"",
    "migrate:dev": "prisma migrate dev",
    "migrate:reset": "prisma migrate reset",
    "migrate:deploy": "prisma migrate deploy",
    "generate": "prisma generate",
    "make:use-case": "plop --plopfile ./.plop/use-case.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.244",
    "@swc/jest": "^0.2.24",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^3.3.0",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.3",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/ms": "^0.7.31",
    "@types/node": "^18.11.17",
    "@types/nodemailer": "^6.4.6",
    "@types/prettier": "^2.7.1",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.0",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "plop": "^2.7.6",
    "prettier": "^2.7.1",
    "prisma": "^4.7.1",
    "rimraf": "^3.0.2",
    "supertest": "^6.3.1",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@prisma/client": "^4.7.1",
    "axios": "^1.2.0",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "bullmq": "^3.5.6",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "handlebars": "^4.7.7",
    "helmet": "^6.0.1",
    "http-status-codes": "^2.2.0",
    "ioredis": "^5.2.4",
    "jsonwebtoken": "^9.0.0",
    "ms": "^2.1.3",
    "nodemailer": "^6.8.0",
    "otplib": "^12.0.1",
    "reflect-metadata": "^0.1.13",
    "tiny-glob": "^0.2.9",
    "uuid": "^9.0.0"
  }
}
